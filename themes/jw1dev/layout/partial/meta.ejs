<% tags = [] %>

<% if(page.__post) {
  page.tags.forEach((el) => {
    tags.push(el.name)
  })
} %>

<meta name="keywords" content="<%- `${config.keywords},${tags.join(',')}` %>" id="keywords" />
<meta name="description" content="<%- config.description %>" />
<%- css(
  [
    'style/reset.css',
    'style/iconfont/iconfont.css',
    'style/style.css',
    'style/tailwind.dist.css'
  ]) %>
<link rel="icon" href="/favicon.ico">
