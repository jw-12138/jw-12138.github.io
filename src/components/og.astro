---
let {path, title, slug} = Astro.props
let image = 'https://jw1.dev/og.jpg'
if (slug) {
  console.log(slug)
  image = "https://jw1.dev/og/" + slug + '.jpg'
}
---

<meta property="og:url" content={"https://jw1.dev" + path}>
<meta property="og:type" content="website">
<meta property="og:title" content={title}>
<meta property="og:description" content="Front-end Developer / Music Producer">
<meta property="og:image" content={image}>

<meta name="X:card" content="summary"></meta>
<meta name="twitter:card" content="summary">
<meta name="twitter:creator" content="@jw1dev">
<meta property="twitter:domain" content="jw1.dev">
