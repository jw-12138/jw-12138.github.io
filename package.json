{
  "name": "jw-12138.github.io",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "hexo clean",
    "serve": "concurrently npm:dev-*",
    "dev-hexo": "ENV=DEV; hexo server --draft",
    "dev-scss": "npx sass themes/jw1dev/source/style:themes/jw1dev/source/style --embed-source-map -w",
    "dev-tailwind": "npx tailwindcss -i themes/jw1dev/source/style/tailwind.css -o themes/jw1dev/source/style/tailwind.dist.css --watch",
    "build": "hexo clean; node audio_meta_gen.js; hexo generate; node readme_gen.js; npm run build-tailwind",
    "build-tailwind": "npx tailwindcss -i themes/jw1dev/source/style/tailwind.css -o themes/jw1dev/source/style/tailwind.dist.css",
    "start": "concurrently npm:dev-*",
    "deploy": "npm run build; cp vercel.json dist/vercel.json; cp -r .vercel dist/.vercel; vercel ./dist --prod --yes; hexo clean",
    "preview": "hexo server -s -p 5100"
  },
  "hexo": {
    "version": "6.0.0"
  },
  "dependencies": {
    "bun-types": "^1.0.1",
    "cssfilter": "^0.0.10",
    "dotenv": "^16.0.3",
    "glob": "^9.0.1",
    "hexo": "6.0.0",
    "hexo-babel": "^1.1.14",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^4.0.0",
    "hexo-server": "^2.0.0",
    "moment-timezone": "^0.5.41",
    "music-metadata": "^8.1.0",
    "walk": "^2.3.15"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "concurrently": "^6.4.0",
    "sass": "^1.45.0",
    "tailwindcss": "^3.3.5"
  }
}
